release: MAIN.ASM READ.OBJ ENTROPY.OBJ EXPHT.OBJ SHOW.OBJ HELPERS.OBJ TRANS.LIB
	ML /W3 /WX READ.OBJ ENTROPY.OBJ EXPHT.OBJ TRANS.LIB SHOW.OBJ HELPERS.OBJ TRANS.LIB MAIN.ASM /Fe MAIN.EXE

debug: MAIN.ASM READD.OBJ ENTROPYD.OBJ SHOWD.OBJ HELPERSD.OBJ EXPHTD.OBJ TRANSD.LIB
	ML /Zi /W3 /WX READD.OBJ ENTROPYD.OBJ SHOWD.OBJ HELPERSD.OBJ EXPHTD.OBJ TRANSD.LIB MAIN.ASM /Fe MAIND.EXE
	
#Library
TRANS.LIB: QUANT.OBJ QUANTT.OBJ IDCT.OBJ IDCTT.OBJ SAMP.OBJ SAMPT.OBJ
	DEL TRANS.LIB
	LIB TRANS.LIB +QUANT.OBJ +QUANTT.OBJ +IDCT.OBJ +IDCTT.OBJ +SAMP.OBJ +SAMPT.OBJ,,
	
TRANSD.LIB: QUANTD.OBJ QUANTTD.OBJ IDCTD.OBJ IDCTTD.OBJ SAMPD.OBJ SAMPTD.OBJ
	DEL TRANSD.LIB
	LIB TRANSD.LIB +QUANTD.OBJ +QUANTTD.OBJ +IDCTD.OBJ +IDCTTD.OBJ +SAMPD.OBJ +SAMPTD.OBJ,,
	
READ.OBJ: READ.ASM
	ML /c /W3 /WX /FoREAD.OBJ /Ta READ.ASM

READD.OBJ: READ.ASM
	ML /c /Zi /W3 /WX /FoREADD.OBJ /Ta READ.ASM

EXPHT.OBJ: EXPHT.ASM
	ML /c /W3 /WX /FoEXPHT.OBJ /Ta EXPHT.ASM
	
EXPHTD.OBJ: EXPHT.ASM
	ML /c /Zi /W3 /WX /FoEXPHTD.OBJ /Ta EXPHT.ASM
	
ENTROPY.OBJ: ENTROPY.ASM
	ML /c /W3 /WX /FoENTROPY.OBJ /Ta ENTROPY.ASM
	
ENTROPYD.OBJ: ENTROPY.ASM
	ML /c /Zi /W3 /WX /FoENTROPYD.OBJ /Ta ENTROPY.ASM

QUANT.OBJ: QUANT.ASM
	ML /c /W3 /WX /FoQUANT.OBJ /Ta QUANT.ASM
	
QUANTD.OBJ: QUANT.ASM
	ML /c /Zi /W3 /WX /FoQUANTD.OBJ /Ta QUANT.ASM
	
QUANTT.OBJ: QUANTT.ASM
	ML /c /W3 /WX /FoQUANTT.OBJ /Ta QUANTT.ASM
	
QUANTTD.OBJ: QUANTT.ASM
	ML /c /Zi /W3 /WX /FoQUANTTD.OBJ /Ta QUANTT.ASM

#IDCT
IDCT.OBJ: IDCT.ASM
	ML /c /W3 /WX /FoIDCT.OBJ /Ta IDCT.ASM
	
IDCTD.OBJ: IDCT.ASM
	ML /c /Zi /W3 /WX /FoIDCTD.OBJ /Ta IDCT.ASM
	
IDCTT.OBJ: IDCTT.ASM
	ML /c /W3 /WX /FoIDCTT.OBJ /Ta IDCTT.ASM
	
IDCTTD.OBJ: IDCTT.ASM
	ML /c /Zi /W3 /WX /FoIDCTTD.OBJ /Ta IDCTT.ASM
	
#Sampling
SAMP.OBJ: SAMP.ASM
	ML /c /W3 /WX /FoSAMP.OBJ /Ta SAMP.ASM
	
SAMPD.OBJ: SAMP.ASM
	ML /c /Zi /W3 /WX /FoSAMPD.OBJ /Ta SAMP.ASM
	
SAMPT.OBJ: SAMPT.ASM
	ML /c /W3 /WX /FoSAMPT.OBJ /Ta SAMPT.ASM
	
SAMPTD.OBJ: SAMPT.ASM
	ML /c /Zi /W3 /WX /FoSAMPTD.OBJ /Ta SAMPT.ASM


SHOW.OBJ: SHOW.ASM
	ML /c /W3 /WX /FoSHOW.OBJ /Ta SHOW.ASM
	
SHOWD.OBJ: SHOW.ASM
	ML /c /Zi /W3 /WX /FoSHOWD.OBJ /Ta SHOW.ASM
	
DATA.OBJ: DATA.ASM
	ML /c /W3 /WX /FoDATA.OBJ /Ta DATA.ASM
	
DATAD.OBJ: DATA.ASM
	ML /c /Zi /W3 /WX /FoDATAD.OBJ /Ta DATA.ASM
	
HELPERS.OBJ: HELPERS.ASM
	ML /c /W3 /WX /FoHELPERS.OBJ /Ta HELPERS.ASM
	
HELPERSD.OBJ: HELPERS.ASM
	ML /c /Zi /W3 /WX /FoHELPERSD.OBJ /Ta HELPERS.ASM
	
clean:
	del *.EXE
	del *.OBJ

cv: debug
	cv /s /43 MAIND.EXE