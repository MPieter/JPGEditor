release: MAIN.ASM READ.OBJ ENTROPY.OBJ  SHIFT.OBJ SHOW.OBJ HELPERS.OBJ TRANS.LIB
	ML /W3 /WX READ.OBJ ENTROPY.OBJ TRANS.LIB SHIFT.OBJ SHOW.OBJ HELPERS.OBJ MAIN.ASM /Fe MAIN.EXE

debug: MAIN.ASM READ.OBJ ENTROPY.OBJ IDCT.OBJ IDCTT.OBJ SHIFT.OBJ SHOW.OBJ HELPERS.OBJ
	ML /Zi /W3 /WX READ.OBJ ENTROPY.OBJ IDCT.OBJ IDCTT.OBJ SHIFT.OBJ SHOW.OBJ HELPERS.OBJ MAIN.ASM /Fe MAIND.EXE
	
TRANS.LIB: QUANT.OBJ QUANTT.OBJ IDCT.OBJ IDCTT.OBJ
	DEL TRANS.LIB
	LIB TRANS.LIB +QUANT.OBJ +QUANTT.OBJ +IDCT.OBJ +IDCTT.OBJ,,
	
READ.OBJ: READ.ASM
	ML /c /W3 /WX /FoREAD.OBJ /Ta READ.ASM

READD.OBJ: READ.ASM
	ML /c /Zi /W3 /WX /FoREADD.OBJ /Ta READ.ASM

EXPANSHT.OBJ: EXPANSHT.ASM
	ML /c /W3 /WX /FoEXPANSHT.OBJ /Ta EXPANSHT.ASM
	
EXPANSHTD.OBJ: EXPANSHT.ASM
	ML /c /Zi /W3 /WX /FoEXPANSHTD.OBJ /Ta EXPANSHT.ASM
	
ENTROPY.OBJ: ENTROPY.ASM
	ML /c /W3 /WX /FoENTROPY.OBJ /Ta ENTROPY.ASM
	
ENTROPYD.OBJ: ENTROPY.ASM
	ML /c /Zi /W3 /WX /FoENTROPYD.OBJ /Ta ENTROPY.ASM

QUANT.OBJ: QUANT.ASM
	ML /c /W3 /WX /FoQUANT.OBJ /Ta QUANT.ASM
	
QUANTD.OBJ: QUANT.ASM
	ML /c /Zi /W3 /WX /FoQUANTD.OBJ /Ta QUANT.ASM
	
QUANTT.OBJ: QUANTT.ASM
	ML /c /W3 /WX /FoQUANTT.OBJ /Ta QUANTT.ASM
	
QUANTTD.OBJ: QUANTT.ASM
	ML /c /Zi /W3 /WX /FoQUANTTD.OBJ /Ta QUANTT.ASM

IDCT.OBJ: IDCT.ASM
	ML /c /W3 /WX /FoIDCT.OBJ /Ta IDCT.ASM
	
IDCTD.OBJ: IDCT.ASM
	ML /c /Zi /W3 /WX /FoIDCTD.OBJ /Ta IDCT.ASM
	
IDCTT.OBJ: IDCTT.ASM
	ML /c /W3 /WX /FoIDCTT.OBJ /Ta IDCTT.ASM
	
IDCTTD.OBJ: IDCTT.ASM
	ML /c /Zi /W3 /WX /FoIDCTTD.OBJ /Ta IDCTT.ASM
	
SHIFT.OBJ: SHIFT.ASM
	ML /c /W3 /WX /FoSHIFT.OBJ /Ta SHIFT.ASM
	
SHIFTD.OBJ: SHIFT.ASM
	ML /c /Zi /W3 /WX /FoSHIFTD.OBJ /Ta SHIFT.ASM
	
SHOW.OBJ: SHOW.ASM
	ML /c /W3 /WX /FoSHOW.OBJ /Ta SHOW.ASM
	
SHOWD.OBJ: SHOW.ASM
	ML /c /Zi /W3 /WX /FoSHOWD.OBJ /Ta SHOW.ASM
	
DATA.OBJ: DATA.ASM
	ML /c /W3 /WX /FoDATA.OBJ /Ta DATA.ASM
	
DATAD.OBJ: DATA.ASM
	ML /c /Zi /W3 /WX /FoDATAD.OBJ /Ta DATA.ASM
	
HELPERS.OBJ: HELPERS.ASM
	ML /c /W3 /WX /FoHELPERS.OBJ /Ta HELPERS.ASM
	
HELPERSD.OBJ: HELPERS.ASM
	ML /c /Zi /W3 /WX /FoHELPERSD.OBJ /Ta HELPERS.ASM
	
clean:
	del *.EXE
	del *.OBJ

cv: debug
	cv /s /43 MAIND.EXE